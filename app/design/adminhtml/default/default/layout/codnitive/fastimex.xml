<layout>
    <fastimex_adminhtml_import_index>
        <reference name="content">
            <block type="adminhtml/template" name="fastimex_import_form" template="codnitive/fastimex/import/form.phtml" />
        </reference>
    </fastimex_adminhtml_import_index>
    <fastimex_adminhtml_import_start>
        <reference name="content">
            <block type="adminhtml/template" name="fastimex_import_form" template="codnitive/fastimex/import/form.phtml" />
        </reference>
    </fastimex_adminhtml_import_start>
    <adminhtml_import_busy>
        <reference name="content">
            <block type="core/template" template="importexport/busy.phtml" name="busy" alias="busy"/>
        </reference>
    </adminhtml_import_busy>
    <fastimex_adminhtml_export_index>
        <reference name="content">
            <block type="adminhtml/template" name="fastimex_export_form" template="codnitive/fastimex/export/form.phtml" />
        </reference>
    </fastimex_adminhtml_export_index>
    <fastimex_adminhtml_export_getfilter>
        <block type="core/text_list" name="root">
            <block type="importexport/adminhtml_export_filter" name="export.filter"/>
        </block>
    </fastimex_adminhtml_export_getfilter>
</layout>
